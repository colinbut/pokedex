<html>
    <body>
        <h1>{{pokemon.id}} - {{pokemon.name}}</h1>
        <table>
            <tr><td><b>Hit Points:</b></td><td>{{pokemon.hitPoints}}</td></tr>
            <tr><td><b>Combat Power:</b></td><td>{{pokemon.combatPower}}</td></tr>
            <tr><td><b>Type:</b></td><td>{{pokemon.type}}</td></tr>
        </table>
        <h2>Attacks:</h2>
        <table>
            <tr><th>Name</th><th>Power</th><th>Accuracy</th><th>Type</th></tr>
            {{#pokemon.attacks}}
                <tr><td>{{name}}</td><td>{{power}}</td><td>{{accuracy}}</td><td>{{type}}</td></tr>
            {{/pokemon.attacks}}
        </table>
        <p>view generated by Mustache</p>
    </body>
</html>
